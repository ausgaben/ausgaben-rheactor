{
  "name": "ausgaben-rheactor",
  "version": "0.3.3",
  "description": "Personal spending build with Node.js",
  "main": "server/server.js",
  "scripts": {
    "start": "node server/server.js",
    "dev-server": "environment=development nodemon -e 'js,html,css' -w ./server -w ./util -d 1 ./server/server.js",
    "test": "environment=testing mocha --require babel-polyfill --compilers js:babel-register --reporter=dot --timeout=1350 test/\\*\\*/\\*.js",
    "test:api": "environment=testing mocha --require babel-polyfill --compilers js:babel-register --reporter=dot --timeout=1350 test/yadda/\\*\\*/\\*.js",
    "test:backend": "environment=testing mocha --require babel-polyfill --compilers js:babel-register --reporter=dot --timeout=1350 test/mocha/\\*\\*/\\*.js",
    "test:coverage": "NODE_ENV=nyc babel server --out-dir instrumented-server > /dev/null && nyc --reporter=text-summary --reporter=lcov npm run test",
    "test:coverage-travis": "npm run test:coverage && codeclimate-test-reporter < ./coverage/lcov.info && rm -rf ./coverage",
    "lint": "standard",
    "precommit": "npm run lint",
    "commitmsg": "validate-commit-msg"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ausgaben/ausgaben-rheactor.git"
  },
  "keywords": [
    "personal",
    "spending",
    "tracker"
  ],
  "author": "Markus Tacker <m@coderbyheart.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ausgaben/ausgaben-rheactor/issues"
  },
  "homepage": "https://github.com/ausgaben/ausgaben-rheactor#readme",
  "dependencies": {
    "@ausgaben/models": "1.x",
    "@rheactorjs/errors": "1.x",
    "@rheactorjs/event-store": "1.x",
    "@rheactorjs/models": "1.x",
    "@rheactorjs/server": "1.x",
    "@rheactorjs/template-mailer-client": "1.x",
    "@rheactorjs/value-objects": "1.x",
    "aws-sdk": "^2.22.0",
    "bcrypt": "^1.0.2",
    "bluebird": "^3.5.0",
    "body-parser": "^1.17.0",
    "colors": "^1.1.2",
    "commander": "^2.9.0",
    "compare-versions": "^3.0.0",
    "cors": "^2.8.3",
    "express": "^4.15.0",
    "glob": "^7.1.1",
    "joi": "^10.2.2",
    "jsonwebtoken": "^7.4.1",
    "keypair": "^1.0.1",
    "lodash": "^4.17.4",
    "moment": "^2.17.1",
    "nconf": "^0.8.4",
    "redis": "^2.7.1",
    "reds": "^0.2.5",
    "request": "^2.80.0",
    "request-promise": "^4.1.1",
    "showdown": "^1.6.4",
    "tcomb": "^3.2.18",
    "tether": "^1.4.0"
  },
  "devDependencies": {
    "@rheactorjs/bdd-contexts": "1.x",
    "@rheactorjs/build-views": "1.x",
    "@rheactorjs/yadda-feature-runner": "1.x",
    "babel-cli": "^6.23.0",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-preset-env": "^1.2.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-register": "^6.23.0",
    "chai": "^3.5.0",
    "codeclimate-test-reporter": "^0.4.1",
    "eslint": "^4.1.1",
    "husky": "^0.13.2",
    "inotifywait": "^1.8.3",
    "lolex": "^1.6.0",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0",
    "nyc": "^10.1.2",
    "simple-mock": "^0.7.3",
    "standard": "^9.0.0",
    "superagent": "^3.5.0",
    "supertest": "^3.0.0",
    "toposort": "^1.0.3",
    "validate-commit-msg": "^2.11.1",
    "yadda": "^1.1.1"
  }
}
