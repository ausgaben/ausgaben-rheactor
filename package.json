{
  "name": "ausgaben-rheactor",
  "version": "0.3.3",
  "description": "Personal spending build with Node.js",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev-server": "environment=development nodemon -e 'js,html,css' -w ./src -w ./util -d 1 ./src/server.js",
    "test": "environment=testing mocha --require babel-polyfill --compilers js:babel-register --reporter=dot --timeout=1350 test/\\*\\*/\\*.js",
    "test:api": "environment=testing mocha --require babel-polyfill --compilers js:babel-register --reporter=dot --timeout=1350 test/yadda/\\*\\*/\\*.js",
    "test:backend": "environment=testing mocha --require babel-polyfill --compilers js:babel-register --reporter=dot --timeout=1350 test/mocha/\\*\\*/\\*.js",
    "test:coverage": "NODE_ENV=nyc babel src --out-dir instrumented-server > /dev/null && nyc --reporter=text-summary --reporter=lcov npm run test",
    "test:coverage-travis": "npm run test:coverage && codeclimate-test-reporter < ./coverage/lcov.info && rm -rf ./coverage",
    "lint": "standard",
    "precommit": "npm run lint",
    "commitmsg": "validate-commit-msg"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ausgaben/ausgaben-rheactor.git"
  },
  "keywords": [
    "personal",
    "spending",
    "tracker"
  ],
  "author": "Markus Tacker <m@coderbyheart.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ausgaben/ausgaben-rheactor/issues"
  },
  "homepage": "https://github.com/ausgaben/ausgaben-rheactor#readme",
  "dependencies": {
    "@ausgaben/models": "^1.1.0",
    "@rheactorjs/errors": "^1.0.0",
    "@rheactorjs/event-store": "^3.0.1",
    "@rheactorjs/models": "^2.0.1",
    "@rheactorjs/server": "^2.2.5",
    "@rheactorjs/template-mailer-client": "^1.0.0",
    "@rheactorjs/value-objects": "^1.0.2",
    "aws-sdk": "^2.97.0",
    "bcrypt": "^1.0.2",
    "bluebird": "^3.5.0",
    "body-parser": "^1.17.0",
    "colors": "^1.1.2",
    "commander": "^2.9.0",
    "compare-versions": "^3.0.0",
    "cors": "^2.8.4",
    "express": "^4.15.4",
    "glob": "^7.1.1",
    "joi": "^13.0.0",
    "jsonwebtoken": "^8.1.0",
    "keypair": "^1.0.1",
    "lodash": "^4.17.4",
    "moment": "^2.17.1",
    "nconf": "^0.8.4",
    "redis": "^2.8.0",
    "reds": "^1.0.0",
    "request": "^2.80.0",
    "request-promise": "^4.2.1",
    "showdown": "^1.7.2",
    "tcomb": "^3.2.22",
    "tether": "^1.4.0"
  },
  "devDependencies": {
    "@rheactorjs/bdd-contexts": "^2.0.0",
    "@rheactorjs/build-views": "^1.0.1",
    "@rheactorjs/yadda-feature-runner": "^2.0.0",
    "babel-cli": "^6.24.1",
    "babel-helper-function-name": "^6.24.1",
    "babel-helper-get-function-arity": "^6.24.1",
    "babel-messages": "^6.23.0",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-plugin-transform-strict-mode": "^6.24.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-register": "^6.23.0",
    "babel-types": "^6.25.0",
    "babylon": "^6.17.4",
    "chai": "^4.1.1",
    "codeclimate-test-reporter": "^0.5.0",
    "eslint": "^4.4.1",
    "husky": "^0.14.3",
    "inotifywait": "^1.8.3",
    "lolex": "^2.1.2",
    "mocha": "^4.0.1",
    "nodemon": "^1.11.0",
    "nyc": "^11.1.0",
    "simple-mock": "^0.8.0",
    "standard": "^10.0.3",
    "superagent": "^3.5.0",
    "supertest": "^3.0.0",
    "toposort": "^1.0.3",
    "validate-commit-msg": "^2.14.0",
    "yadda": "^1.4.0"
  },
  "engines": {
    "node": ">=6.10.2"
  }
}
