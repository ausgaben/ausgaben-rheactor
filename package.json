    "bootstrap": "./node_modules/bootstrap/dist/js/bootstrap.js",
    "bluebird": "./node_modules/bluebird/js/browser/bluebird.js",
    "moment": "./node_modules/moment/moment.js",
    "angular-moment": "./node_modules/angular-moment/angular-moment.js"
  },
  "browserify-shim": {
    "jquery": {
      "exports": "$"
    },
    "angular": {
      "exports": "angular",
      "depends": "jquery"
    },
    "angular-ui-router": {
      "depends": "angular"
    },
    "angular-validation-match": {
      "depends": "angular"
    },
    "angular-sanitize": {
      "depends": "angular"
    },
    "bootstrap": {
      "exports": "bootstrap",
      "depends": [
        "tether",
        "jquery"
      ]
    },
    "tether": "tether"
  },
